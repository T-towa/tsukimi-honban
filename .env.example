# Supabase設定
# Supabaseプロジェクトの情報をここに設定してください
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Claude API設定
# Anthropic Claude APIの設定（月歌生成に使用）
REACT_APP_CLAUDE_API_KEY=your_claude_api_key_here
REACT_APP_CLAUDE_API_URL=https://api.anthropic.com/v1/messages
REACT_APP_CLAUDE_MODEL=claude-sonnet-4-20250514

# アプリケーション設定
# 選択可能な感想の最大数
REACT_APP_MAX_FEELINGS=3

# カスタム感想の最大文字数
REACT_APP_CUSTOM_FEELING_MAX_LENGTH=50

# 履歴表示の最大件数
REACT_APP_HISTORY_LIMIT=10
